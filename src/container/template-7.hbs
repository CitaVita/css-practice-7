<header class="page_block row g-0">
    <div class="header_logo col-3">
        {{> logo-2}}
    </div>
    
{{#with header}}

  <nav class="header_nav col-9">
      <div class="header_nav_right"> 

        {{!-- {{> nav-2 list=links}} --}}
        <div class="nav {{#if isVertical}}nav_vertical{{else}}nav_inline{{/if}}"></div>
         {{#each links}}
             <a href="{{url}}" class="nav_links">{{text}}</a>
        {{/each}}
        </div>   
   </nav> 

<div class="header_brandName row g-0">
    <div class="logo-ad col-3">
        <span class="bage">Ad</span>
    </div>
    <div class="ad col-6">
       {{#with ad}}
        <a href="{{url}}" class="ad_text">{{text}}</a>    
    {{/with}} 
    </div>
    <div class="user_online">User Online:{{userOnline}}</div>
</div>
{{/with}}
</header>

  



   







    <footer class="page_block">
             {{> divider }}
                <div class="row  g-0 footer">
                    <div class="col-3">
                   
                    <span class="footer_copyright">Copyright 2024</span>
                    </div>  
                {{#each footer}} 
                <div class="col-3">
                    <strong>
                    <span class="footer_title">{{title}}</span>
                    </strong>
                     {{> nav list=links isVertical=true }}
                </div>
                {{/each}}
                </div>
    </footer>      

